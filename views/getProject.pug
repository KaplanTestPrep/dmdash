extends layout

include mixins/_progressBar

block content
    .card
        .card-content
            .toolbar
                button.btn.btn-default#hapyCreateProj Create Project
                button.btn.btn-default#hapyDeleteProj Delete Project
            .fresh-datatables
                table#hapyakProjects.table.table-striped.table-no-bordered.table-hover
                    thead
                        tr
                            th User
                            th Topic
                            th Start Time
                            th File Type
                            th Download URL
                    tfoot
                        tr
                            th User
                            th Topic
                            th Start Time
                            th File Type
                            th Download URL
