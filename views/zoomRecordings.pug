extends layout


block content
    .card
        .card-content
            .toolbar
                .buttons
                    button.btn.btn-default#test Show selected
                    button.btn.btn-default#delete Delete Selected (1)
            .fresh-datatables
            table#datatables.table.table-striped.table-no-bordered.table-hover
